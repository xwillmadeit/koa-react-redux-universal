{
	"extends": "airbnb",
	"env": {
        "browser": true
    },
	"rules": {
		"semi": 0,
		"react/jsx-filename-extension": [1, { "extensions": [".js"] }],
		"no-tabs": 0,
		"react/jsx-indent": 0,
		"indent": 0,
		"no-mixed-spaces-and-tabs": 0,
		"comma-dangle": 0,
		"arrow-body-style": 0,
		"arrow-parens": 1,
		"class-methods-use-this": 0,
		"import/prefer-default-export": 1,
		"no-trailing-spaces": 1
	}
}