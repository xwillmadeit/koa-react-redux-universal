{
  "name": "koa-rendering-react",
  "version": "0.0.1",
  "description": "Universal React Multi-Page App w/ koa2",
  "main": "index.js",
  "keywords": [
    "universal",
    "react",
    "redux",
    "react-router",
    "koa2",
    "webpack-isomorphic"
  ],
  "author": "xwillmadeit <xiesenzhengyi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:server dev:webpack",
    "dev:client": "better-npm-run client:dev",
    "dev:server": "better-npm-run dev:server",
    "dev:webpack": "better-npm-run dev:webpack",
    "prebuild:client": "rimraf server/public/build",
    "build:client": "better-npm-run build:client",
    "build": "npm-run-all build:client compile",
    "compile": "npm-run-all compile:client compile:server",
    "compile:client": "babel client -d dist/client --ignore client/images client/styles",
    "postcompile:client": "npm run copy:assets",
    "compile:server": "babel server -d dist/server --ignore server/public",
    "postcompile:server": "npm run copy:public",
    "postbuild": "npm run copy:config",
    "copy:public": "ncp server/public dist/server/public",
    "copy:config": "ncp webpack-assets.json dist/webpack-assets.json && ncp isomorphic-config.js dist/isomorphic-config.js",
    "copy:assets": "npm-run-all --parallel copy:images copy:styles",
    "copy:images": "babel client/images -d dist/client/images --copy-files",
    "copy:styles": "babel client/styles -d dist/client/styles --copy-files",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "start": {
      "command": "cross-env node dist/server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "client:dev": {
      "command": "cross-env babel-node server/webpack.devServer.js --client=only",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "client"
      }
    },
    "dev:server": {
      "command": "cross-env nodemon server/index.js --exec babel-node",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:webpack": {
      "command": "cross-env babel-node server/webpack.devServer.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:client": {
      "command": "cross-env webpack -p",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "antd": "^2.8.3",
    "axios": "^0.15.3",
    "babel-polyfill": "^6.23.0",
    "koa": "^2.0.1",
    "koa-bodyparser": "^4.1.0",
    "koa-favicon": "^2.0.0",
    "koa-jwt": "^3.0.0",
    "koa-logger": "^2.0.1",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0-beta.8",
    "redux": "^3.6.0",
    "serialize-javascript": "^1.3.0",
    "autoprefixer": "^6.7.7",
    "webpack-isomorphic-tools": "^3.0.2"
  },
  "devDependencies": {

    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "better-npm-run": "0.0.14",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "koa-webpack": "^0.3.1",
    "minimist": "^1.2.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  }
}
