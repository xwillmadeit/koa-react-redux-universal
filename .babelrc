{
	"env": {
		"client": {
			"presets": [
				["env", {"modules": false }],
				"react"
			],
			"plugins": [
				"transform-object-rest-spread",
				"transform-class-properties",
        ["import", { "libraryName": "antd", "style": "css" }],
        "react-hot-loader/babel"
			]
		},
		"development": {
			"presets": [
				"env",
				"react"
			],
			"plugins": [
				"transform-object-rest-spread",
				"transform-class-properties",
				"add-module-exports"
			]
		},
		"production": {
			"presets": [
				"env",
				"react"
			],
			"plugins": [
				"transform-object-rest-spread",
				"transform-class-properties",
				"add-module-exports"
			]
		}
	}
}
